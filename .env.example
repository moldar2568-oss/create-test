# backend/.env.example
# Dropbox設定
DROPBOX_ACCESS_TOKEN=your_dropbox_access_token_here

# データベース設定（Renderでは自動設定される）
DATABASE_URL=postgresql://user:password@host:port/database

# Dropboxフォルダパス
DROPBOX_TEST_FOLDER=/過去問DB
DROPBOX_QUESTION_FOLDER=/問題集PDF

# その他
TEMP_DIR=/tmp
OUTPUT_DIR=./generated_tests

# Render設定
RENDER_FRONTEND_URL=https://test-generator-frontend.onrender.com